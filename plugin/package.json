{
  "name": "WebProtection",
  "version": "1.0.0",
  "author": "Web Protection",
  "description": "Web Protection",
  "scripts": {
    "build:chrome": "webpack --config chrome/webpack.config.js",
    "build:chrome-production": "webpack -p --config chrome/webpack.config.prod.js ",
    "build:wefinex-production": "webpack -p --config wefinex-helper/webpack.config.prod.js ",
    "watch:chrome": "webpack --config chrome/webpack.config.js -w",
    "build:production": "rimraf prod/src && npm run build:chrome-production && npm run pack",
    "build:production-wefinex": "rimraf wefinex-helper/prod/src && npm run build:wefinex-production && npm run pack:wefinex",
    "build": "rimraf prod/src && npm run build:chrome ",
    "pack": "cd prod/ && bestzip ../bot-wefinex.zip *",
    "pack:wefinex": "cd wefinex-helper/prod/ && bestzip ../../wefinex-helper.zip *"
  },
  "private": true,
  "dependencies": {
    "firebase": "^8.2.2"
  },
  "devDependencies": {
    "@types/chrome": "0.0.81",
    "@types/node": "^12.12.32",
    "awesome-typescript-loader": "^5.2.1",
    "bestzip": "^2.1.7",
    "codelyzer": "^6.0.0",
    "javascript-obfuscator": "^2.10.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-obfuscator": "^3.3.0"
  }
}
